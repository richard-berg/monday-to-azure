{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "GAGGLE_API_KEY": "3856f21f-ad48-4cb2-bdb5-2008f32cf263"
      }
    },
    {
      "name": "Attach to Python Functions",
      "type": "debugpy",
      "request": "attach",
      "connect": { "port": 9091 },
      "preLaunchTask": "func: host start"
    }
  ]
}
